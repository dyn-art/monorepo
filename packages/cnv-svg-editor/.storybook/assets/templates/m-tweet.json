				"        opacity = alpha",
				"    }",
				"})"
			]
		},
		{
			"id": "profilePicture",
			"displayName": "Profile Picture",
			"inputVariant": {
				"type": "PAINT",
				"default": { "paint": { "type": "Solid", "color": [255, 219, 219] }, "opacity": 1 }
			},
			"source": [
				"function mergeTables(dest, source)",
				"    if source ~= nil then",
				"        for key, value in pairs(source) do",
				"            dest[key] = value",
				"        end",
				"    end",
				"    return dest",
				"end",

				"local paint = {",
				"    id = \"p6\",",
				"}",

				"-- Merge args.paint into paint",
				"paint = mergeTables(paint, args.paint)",

				"local content = args.content",
				"if content ~= nil then",
				"    cnv.sendEvent({",
				"        type = \"CreateAsset\",",
				"        asset = {",
				"            id = \"a99\",",
				"            content = { type = \"Binary\", content = content },",
				"            contentType = { type = \"Png\" }",
				"        }",
				"    })",
				"    paint.imageId = { type = \"ReferenceId\", referenceId = \"a99\" }",
				"end",

				"cnv.sendEvents({",
				"    {",
				"        type = \"CreatePaint\",",
				"        paint = paint",
				"    },",
				"    {",
				"        type = \"UpdateFillStyle\",",
				"        id = { type = \"ReferenceId\", referenceId = \"s5\" },",
				"        paintId = { type = \"ReferenceId\", referenceId = \"p6\" }",
				"    }",
				"})",

				"local opacity = args.opacity",
				"if opacity ~= nil then",
				"    cnv.sendEvent({",
				"        type = \"UpdateEntityOpacity\",",
				"        id = { type = \"ReferenceId\", referenceId = \"s5\" },",
				"        opacity = opacity",
				"    })",
				"end"
			]
		},
		{
			"id": "date",
			"displayName": "Date",
			"inputVariant": { "type": "DATETIME", "default": "NOW", "withTime": true },
			"source": [
				"local date = args.input",
				"local dateObj = cnv.date.parse(date)",
				"local hours = dateObj.hour",
				"local minutes = dateObj.minute",
				"local ampm = hours >= 12 and 'PM' or 'AM'",
				"hours = hours % 12",
				"hours = hours ~= 0 and hours or 12",
				"local minutesStr = minutes < 10 and '0' .. minutes or minutes",
				"local timeStr = hours .. \":\" .. minutesStr .. \" \" .. ampm",
				"local dateStr = cnv.date.format('%b %d, %Y', date)",
				"local combinedStr = timeStr .. \" · \" .. dateStr",
				"",
				"cnv.sendEvent({",
				"    type = \"UpdateTextNode\",",
				"    id = { type = \"ReferenceId\", referenceId = \"n19\" },",
				"    text = combinedStr",
				"})"
			]
		}
