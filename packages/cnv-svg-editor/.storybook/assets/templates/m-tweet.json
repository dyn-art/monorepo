			"source": [
				"local rgb = { args.r, args.g, args.b }",
				"local alpha = args.a",
				"cnv.sendEvents({",
				"    {",
				"        type = \"UpdateSolidPaint\",",
				"        id = { type = \"ReferenceId\", referenceId = \"p25\" },",
				"        color = rgb",
				"    },",
