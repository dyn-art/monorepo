				"cornerRadii": [0, 0, 0, 0],
				"blendMode": "Normal",
				"opacity": 1,
				"layoutElement": {
					"type": "Absolute",
					"constraints": { "horizontal": "Start", "vertical": "Start" }
				},
				"styles": [
					{ "type": "Fill", "paintId": "red", "blendMode": "Normal", "opacity": 1, "visible": true }
				]
			},
			"square": {
				"type": "Rectangle",
				"visible": true,
				"size": [100, 100],
				"translation": [200, 200],
				"rotationDeg": -3.1805546814635168e-15,
				"cornerRadii": [0, 0, 0, 0],
				"blendMode": "Normal",
				"opacity": 1,
				"layoutElement": {
					"type": "Absolute",
					"constraints": { "horizontal": "Center", "vertical": "Center" }
				},
				"styles": [
					{
						"type": "Stroke",
						"width": 4,
						"paintId": "black",
						"blendMode": "Normal",
						"opacity": 1,
						"visible": true
					},
					{
						"type": "Fill",
						"paintId": "gray",
						"blendMode": "Normal",
						"opacity": 1,
						"visible": true
					}
				]
			}
		},
		"paints": {
			"gray": { "type": "Solid", "color": [217, 217, 217] },
			"black": { "type": "Solid", "color": [0, 0, 0] },
			"red": { "type": "Solid", "color": [255, 219, 219] }
		},
		"assets": {},
		"viewport": { "physicalPosition": [0, 0], "physicalSize": [500, 500] }
	},
	"modificationFields": [
		{
			"key": "pos",
			"displayName": "Set Position",
			"inputType": { "type": "POSITION", "default": [200, 200] },
			"actions": [
				{
					"conditions": [
						{
							"condition": { ">=": [{ "var": "pos.0" }, 0] },
							"notMetMessage": "X can not be negative!"
						},
						{
							"condition": { ">=": [{ "var": "pos.1" }, 0] },
							"notMetMessage": "Y can not be negative!"
						}
					],
					"events": [
						{
							"type": "UpdateEntityPosition",
							"entity": "square",
							"x": { "var": "pos.0" },
							"y": { "var": "pos.1" }
						}
					]
				}
			]
		},
		{
			"key": "rotation",
			"displayName": "Set Rotation",
			"inputType": { "type": "NUMBER", "default": 0 },
			"actions": [
				{
					"conditions": [
						{
							"condition": { ">=": [{ "var": "rotation" }, 0] },
							"notMetMessage": "Rotation can not be negative!"
						},
						{
							"condition": { "<=": [{ "var": "rotation" }, 360] },
							"notMetMessage": "Rotation can not be larger than 360!"
						}
					],
					"events": [
						{
							"type": "UpdateEntityRotation",
							"entity": "square",
							"rotationDeg": { "var": "rotation" }
						}
					]
				}
			]
		}
	]
