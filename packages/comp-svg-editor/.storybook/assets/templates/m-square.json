					"lua": [
						"function modifyPosition(pos)",
						"  if pos[1] < 0 then",
						"    return { error = 'X can not be negative!' }",
						"  elseif pos[2] < 0 then",
						"    return { error = 'Y can not be negative!' }",
						"  else",
