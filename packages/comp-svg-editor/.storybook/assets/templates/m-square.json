				{
					"conditions": [
						{
							"condition": { ">=": [{ "var": "pos.0" }, 0] },
							"notMetMessage": "X can not be negative!"
						},
						{
							"condition": { ">=": [{ "var": "pos.1" }, 0] },
							"notMetMessage": "Y can not be negative!"
						}
					],
					"events": [
						{
							"type": "UpdateEntityTransform",
							"id": { "type": "ReferenceId", "referenceId": "n2" },
							"x": { "var": "pos.0" },
							"y": { "var": "pos.1" }
