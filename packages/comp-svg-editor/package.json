{
	"name": "@dyn/comp-svg-editor",
	"description": "SVG based editor for dyn.art",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "shx rm -rf dist && pnpm run build:css && ../../scripts/cli.sh bundle",
		"build:css": "tailwindcss -i ./src/styles/root.scss -o ./dist/styles.css",
		"build-storybook": "storybook build",
		"storybook": "storybook dev -p 6006",
		"start:dev": " pnpm run watch:css && tsc -w",
		"watch:css": "pnpm run build:css --watch",
		"lint": "eslint --ext .js,.ts src/",
		"clean": "shx rm -rf dist && shx rm -rf node_modules && shx rm -rf .turbo",
		"install:clean": "pnpm run clean && pnpm install",
		"test": "echo \"Error: no test specified\" && exit 1",
		"update:deps": "pnpm update --latest"
	},
	"source": "./src/index.ts",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/dyndotart/monorepo.git"
	},
	"keywords": [],
	"author": "@bennoinbeta",
	"license": "Commercial License",
	"bugs": {
		"url": "https://github.com/dyndotart/monorepo/issues"
	},
	"homepage": "https://dyn.art/?source=github",
	"dependencies": {
		"@dyn/comp-dtif": "workspace:*",
		"@dyn/comp-svg-builder": "workspace:*",
		"@dyn/ui": "workspace:*"
	},
	"peerDependencies": {
		"@tanstack/react-query": "^5.28.9",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@dyn/config": "workspace:*",
		"@storybook/addon-essentials": "^8.0.10",
		"@storybook/addon-interactions": "^8.0.10",
		"@storybook/addon-links": "^8.0.10",
		"@storybook/blocks": "^8.0.10",
		"@storybook/react": "^8.0.10",
		"@storybook/react-vite": "^8.0.10",
		"@storybook/test": "^8.0.10",
		"@tanstack/react-query": "^5.35.1",
		"@types/node": "^20.12.10",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.0",
		"autoprefixer": "^10.4.19",
		"eslint-plugin-storybook": "^0.8.0",
		"postcss": "^8.4.38",
		"postcss-nested": "^6.0.1",
		"react": "^18.3.1",
		"rollup-plugin-preserve-directives": "^0.2.0",
		"scss": "^0.2.4",
		"storybook": "^8.0.10",
		"tailwindcss": "^3.4.3",
		"vite": "^5.2.11",
		"vite-tsconfig-paths": "^4.3.2"
	},
	"files": [
		"dist",
		"README.md"
	]
}
