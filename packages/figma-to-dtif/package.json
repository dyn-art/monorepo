	"license": "AGPL-3.0-or-later",
	"bugs": {
		"url": "https://s.dyn.art/github/issues"
	},
	"homepage": "https://s.dyn.art/github",
	"dependencies": {
		"@dyn/utils": "workspace:*"
	},
	"devDependencies": {
		"@dyn/config": "workspace:*",
		"@dyn/svg-composition": "workspace:*",
		"@figma/plugin-typings": "^1.82.0",
		"@types/node": "^20.8.10"
	},
	"files": [
		"dist",
		"README.md"
