{
	"name": "@dyn/cli",
	"description": "CLI tool for dyn.art monorepo",
	"version": "0.0.1",
	"private": true,
	"bin": {
		"dyn-cli": "./bin/run"
	},
	"scripts": {
		"build": "shx rm -rf dist && tsc",
		"start:dev": "tsc -w",
		"lint": "eslint --ext .js,.ts,.jsx,.tsx src/",
		"clean": "shx rm -rf dist && shx rm -rf node_modules && shx rm -rf .turbo",
		"install:clean": "pnpm run clean && pnpm install",
		"test": "echo \"Error: no test specified\" && exit 1",
		"update:deps": "pnpm update --latest"
	},
	"source": "./src/index.ts",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/dyndotart/monorepo.git"
	},
	"keywords": [],
	"author": "@bennoinbeta",
	"license": "AGPL-3.0-or-later",
	"bugs": {
		"url": "https://s.dyn.art/github/issues"
	},
	"homepage": "https://s.dyn.art/github",
	"dependencies": {
		"@dyn/utils": "workspace:^",
		"@oclif/core": "^3.9.2",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-html": "^1.0.3",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-replace": "^5.0.5",
		"chalk": "^4.1.2",
		"dotenv": "^16.3.1",
		"execa": "^8.0.1",
		"figlet": "^1.7.0",
		"lodash": "^4.17.21",
		"maxmin": "^4.0.0",
		"rollup": "^4.2.0",
		"rollup-plugin-bundle-size": "^1.0.3",
		"rollup-plugin-esbuild": "^6.1.0",
		"rollup-plugin-node-externals": "^5.1.3",
		"rollup-plugin-postcss": "^4.0.2"
	},
	"devDependencies": {
		"@dyn/config": "workspace:*",
		"@types/figlet": "^1.5.7",
		"@types/lodash": "^4.14.200",
		"@types/node": "^20.8.10",
		"type-fest": "^4.6.0"
	},
	"files": [
		"dist",
		"bin",
		"oclif.manifest.json",
		"README.md"
	],
	"oclif": {
		"bin": "dyn-cli",
		"dirname": "dyn-cli",
		"commands": "./dist/commands",
		"topicSeparator": " ",
		"topics": {
			"bundle": {
				"description": "Bundle dyn.art packages"
			}
		}
	}
}
