{
	"version": "V000001",
	"extension": "MDTIF",
	"size": [500, 500],
	"nodes": [
		{
			"type": "Rectangle",
			"visible": true,
			"size": [100, 100],
			"translation": [200, 200],
			"rotationDeg": 0,
			"cornerRadii": [0, 0, 0, 0],
			"blendMode": "Normal",
			"opacity": 1,
			"layoutElement": {
				"type": "Absolute",
				"constraints": { "horizontal": "Center", "vertical": "Center" }
			},
			"styles": [
				{
					"type": "Stroke",
					"width": 4,
					"paintId": { "type": "ReferenceId", "referenceId": "p4" },
					"blendMode": "Normal",
					"opacity": 1,
					"visible": true
				},
				{
					"type": "Fill",
					"paintId": { "type": "ReferenceId", "referenceId": "p3" },
					"blendMode": "Normal",
					"opacity": 1,
					"visible": true
				}
			],
			"id": "n2"
		},
		{
			"type": "Frame",
			"clipContent": false,
			"layoutParent": null,
			"visible": true,
			"children": [{ "type": "ReferenceId", "referenceId": "n2" }],
			"size": [500, 500],
			"translation": [0, 0],
			"rotationDeg": 0,
			"cornerRadii": [0, 0, 0, 0],
			"blendMode": "Normal",
			"opacity": 1,
			"layoutElement": {
				"type": "Absolute",
				"constraints": { "horizontal": "Start", "vertical": "Start" }
			},
			"styles": [
				{
					"type": "Fill",
					"paintId": { "type": "ReferenceId", "referenceId": "p5" },
					"blendMode": "Normal",
					"opacity": 1,
					"visible": true
				}
			],
			"id": "n1"
		}
	],
	"paints": [
		{ "type": "Solid", "color": [217, 217, 217], "id": "p3" },
		{ "type": "Solid", "color": [0, 0, 0], "id": "p4" },
		{ "type": "Solid", "color": [255, 219, 219], "id": "p5" }
	],
	"assets": [],
	"viewport": { "physicalPosition": [0, 0], "physicalSize": [500, 500] },
	"scripts": [
		{
			"id": "pos",
			"displayName": "Set Position",
			"inputVariant": { "type": "POSITION", "default": { "x": 200, "y": 200 } },
			"source": [
				"local x = args.x",
				"local y = args.y",
				"if x < 0 then",
				"    error(\"X can not be negative!\")",
				"end",
				"if y < 0 then",
				"    error(\"Y can not be negative!\")",
				"end",
				"comp.sendEvent({",
				"    type = \"UpdateEntityTransform\",",
				"    id = { type = \"ReferenceId\", referenceId = \"n2\" },",
				"    x = x,",
				"    y = y",
				"})"
			]
		},
		{
			"id": "rotation",
			"displayName": "Set Rotation",
			"inputVariant": { "type": "NUMBER", "default": { "input": 0 } },
			"source": [
				"local rotation = args.input",
				"if rotation < 0 then",
				"    error(\"Rotation can not be negative!\")",
				"end",
				"if rotation > 360 then",
				"    error(\"Rotation can not be larger than 360!\")",
				"end",
				"comp.sendEvent({",
				"    type = \"UpdateEntityRotation\",",
				"    id = { type = \"ReferenceId\", referenceId = \"n2\" },",
				"    rotationDeg = rotation",
				"})"
			]
		}
	]
}
