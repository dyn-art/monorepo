{
	"name": "@dyn/figma-plugin",
	"description": "UI library for dyn.art",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "shx rm -rf dist && ../../scripts/cli.sh bundle figma",
		"start:dev": "tsc -w",
		"lint": "eslint --ext .js,.ts src/",
		"clean": "shx rm -rf dist && shx rm -rf node_modules && shx rm -rf .turbo",
		"install:clean": "pnpm run clean && pnpm install",
		"test": "echo \"Error: no test specified\" && exit 1",
		"update:deps": "pnpm update --latest"
	},
	"exports": {
		"app": {
			"source": "./src/app/index.tsx",
			"previewSource": "./dev/app/index.tsx",
			"require": "./dist/app.js",
			"env": "./.env.app"
		},
		"plugin": {
			"source": "./src/plugin/index.ts",
			"previewSource": "./src/plugin/index.ts",
			"require": "./dist/plugin.js",
			"env": "./.env.plugin"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dyndotart/monorepo.git"
	},
	"keywords": [],
	"author": "@bennoinbeta",
	"license": "AGPL-3.0-or-later",
	"bugs": {
		"url": "https://s.dyn.art/github/issues"
	},
	"homepage": "https://s.dyn.art/github",
	"dependencies": {
		"@dyn/ui": "workspace:*",
		"clsx": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@dyn/config": "workspace:*",
		"@figma/plugin-typings": "^1.82.0",
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.33",
		"@types/react-dom": "^18.2.14",
		"autoprefixer": "^10.4.16",
		"postcss": "^8.4.32",
		"postcss-preset-env": "^9.3.0",
		"tailwindcss": "^3.3.5"
	},
	"files": [
		"dist",
		"README.md"
	]
}
